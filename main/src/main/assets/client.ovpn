client
remote 43.201.70.142 1194
proto tcp
dev tun

# client key 사용안함
# verify-client-cert none #'client-cert-not-required' is old version. # this option is only for server. so commented

# id/pass 방식 사용
auth-user-pass

tun-mtu 1400

remote-cert-tls server
key-direction 1
auth SHA512
cipher AES-256-CBC
data-ciphers AES-256-CBC
# cipher AES-256-CBC : with this option, warning came out and can't connect to server
# so replaced with this below one

resolv-retry infinite
nobind
keepalive 10 60
#persist-tun
persist-key
#redirect-gateway def1 bypass-dhcp
#dhcp-option DNS 8.8.8.8
#dhcp-option DNS 8.8.4.4

